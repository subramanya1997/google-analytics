# Configuration Files
SUPABASE_CONFIG_FILE=config/supabase.json
BIGQUERY_CONFIG_FILE=config/bigquery.json

# Database Configuration
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=0

# Service Configuration
SERVICE_NAME=data-ingestion-service
SERVICE_VERSION=0.1.0
DEBUG=true
LOG_LEVEL=INFO

# API Configuration
API_V1_STR=/api/v1
HOST=0.0.0.0
PORT=8001

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000", "http://localhost:3001"]

# Default BigQuery Configuration (can be overridden in bigquery.json)
GOOGLE_CLOUD_PROJECT_ID=learned-maker-366218
BIGQUERY_DATASET_ID=analytics_349447920

# SFTP Configuration (optional - can be configured per tenant)
SFTP_HOST=your-sftp-host.com
SFTP_PORT=22
SFTP_USERNAME=your-username
SFTP_PASSWORD=your-password
SFTP_REMOTE_PATH=/path/to/files

# Job Processing Configuration
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT_MINUTES=60
CLEANUP_COMPLETED_JOBS_AFTER_DAYS=30

# Security
SECRET_KEY=your-super-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External Service URLs
AUTH_SERVICE_URL=http://localhost:8000
ANALYTICS_SERVICE_URL=http://localhost:8002
